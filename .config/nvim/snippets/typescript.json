{
  "clg": {
    "prefix": "clg",
    "body": [
      "console.log('$1:', $1);"
    ],
    "description": "Console log with variable name and value"
  },
  "interface": {
    "prefix": "interface",
    "body": [
      "interface ${1:Name} {",
      "\t$2",
      "}"
    ],
    "description": "TypeScript interface"
  },
  "type": {
    "prefix": "type",
    "body": [
      "type ${1:Name} = $2;"
    ],
    "description": "TypeScript type alias"
  }
}
